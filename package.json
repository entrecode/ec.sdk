{
  "name": "ec.sdk",
  "version": "0.31.1",
  "description": "SDK for ec.api",
  "repository": "entrecode/ec.sdk",
  "author": "Simon Scherzinger <scherzinger@entrecode.de>",
  "license": "ISC",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "browser": "./dist/ec.sdk.min.js",
  "scripts": {
    "release": "./release.sh $3",
    "build": "npm run buildNode && npm run buildBrowser",
    "buildNode": "tsc",
    "buildBrowser": "cp lib/index.js lib/browser.js && browserify lib/browser.js -o dist/ec.sdk.js -s ec --global-transform [ babelify --presets [ es2015 ] ] && uglifyjs dist/ec.sdk.js --compress --mangle  -o dist/ec.sdk.min.js",
    "changelog": "conventional-changelog -i CHANGELOG.md -s",
    "coveralls": "npm run buildNode && istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R dot --exit --recursive && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "coverage": "npm run buildNode && istanbul cover ./node_modules/mocha/bin/_mocha -- --exit -R dot --recursive && open ./coverage/lcov-report/index.html",
    "docs": "npm run buildNode && documentation build lib/*.js -c documentation.yml -f html -o docs",
    "pretest": "npm run buildNode",
    "test": "mocha -R dot --recursive --exit",
    "pretestTravis": "npm run build",
    "testTravis": "mocha -R dot --recursive --exit",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@types/halfred": "^2.0.1",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^17.0.25",
    "@types/shortid": "^0.0.29",
    "@types/superagent": "^4.1.15",
    "@types/traverson": "^6.1.0",
    "browser-cookies": "^1.2.0",
    "deep-equal": "^2.0.5",
    "ec.errors": "^0.2.10",
    "es6-symbol": "^3.1.3",
    "halfred": "^2.0.0",
    "json-schema-remote": "^1.6.2",
    "jwt-decode": "^2.2.0",
    "shiro-trie": "^0.4.10",
    "shortid": "^2.2.16",
    "stream": "^0.0.2",
    "superagent": "^7.1.4",
    "traverson": "^7.0.1",
    "traverson-hal": "^7.0.2",
    "validator": "^13.7.0"
  },
  "peerDependencies": {
    "eventsource": "^1.0.7"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.5.1",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "conventional-changelog-cli": "^2.2.2",
    "coveralls": "^3.1.1",
    "documentation": "^13.2.5",
    "ec.cookie-mock": "^1.0.1",
    "eslint-config-entrecode": "^2.0.0",
    "eventsource": "^1.0.7",
    "istanbul": "^0.4.5",
    "mocha": "^9.2.2",
    "mocha-bamboo-reporter": "^1.1.2",
    "nock": "^12.0.3",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.7.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "uglify-js": "^3.15.4",
    "webpack": "^4.43.0"
  },
  "eslintConfig": {
    "extends": "entrecode"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
