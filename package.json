{
  "name": "ec.sdk",
  "version": "0.16.7",
  "description": "SDK for ec.api",
  "repository": "entrecode/ec.sdk",
  "author": "Simon Scherzinger <scherzinger@entrecode.de>",
  "license": "ISC",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "release": "./release.sh $3",
    "build": "npm run buildNode && npm run buildBrowser",
    "buildNode": "tsc",
    "buildBrowser": "browserify lib/index.js -o dist/ec.sdk.js -s ec --global-transform [ babelify --presets [ es2015 ] ] && uglifyjs dist/ec.sdk.js --screw-ie8 --compress --mangle  -o dist/ec.sdk.min.js",
    "changelog": "conventional-changelog -i CHANGELOG.md -s",
    "coveralls": "npm run buildNode && istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R dot --exit --recursive && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "coverage": "npm run buildNode && istanbul cover ./node_modules/mocha/bin/_mocha -- --exit -R dot --recursive && open ./coverage/lcov-report/index.html",
    "docs": "documentation build lib/** -c documentation.yml -f html -o docs",
    "pretest": "npm run buildNode",
    "test": "mocha -R dot --recursive --exit",
    "pretestTravis": "npm run build",
    "testTravis": "mocha -R dot --recursive --exit",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@types/halfred": "^1.0.2",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^10.0.3",
    "@types/shortid": "0.0.29",
    "@types/superagent": "^3.5.7",
    "@types/traverson": "^2.0.28",
    "@types/validator": "^9.4.0",
    "browser-cookies": "^1.1.0",
    "ec.errors": "^0.2.2",
    "eventsource": "^1.0.5",
    "halfred": "^1.0.0",
    "json-schema-remote": "^1.3.6",
    "jwt-decode": "^2.2.0",
    "locale": "^0.1.0",
    "lodash.isequal": "^4.5.0",
    "shiro-trie": "^0.4.5",
    "shortid": "^2.2.8",
    "stream": "^0.0.2",
    "superagent": "^3.8.1",
    "traverson": "^6.0.3",
    "traverson-hal": "^6.0.0",
    "validator": "^10.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.0.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "conventional-changelog-cli": "^2.0.0",
    "coveralls": "^3.0.0",
    "documentation": "^7.0.0",
    "ec.cookie-mock": "^1.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.1",
    "mocha-bamboo-reporter": "^1.1.1",
    "nock": "^9.1.0",
    "sinon": "^5.0.0",
    "sinon-chai": "^3.0.0",
    "ts-loader": "^3.5.0",
    "typescript": "^2.8.1",
    "uglify-js": "^3.3.18",
    "webpack": "^4.5.0"
  }
}
